{
  "name": "<%= props.projectName %>",
  "version": "1.0.0",
  "dependencies": {
    "angular-animate": "~1.6.2",
    "angular-sanitize": "~1.6.2",
    "angular-ui-router": "~0.4.2",
    "angular": "~1.6.2",
    "lodash": "~4.17.0",
    "angular-gettext": "~2.3.0",
<% if (props.target !== 'web') { -%>
    "ngCordova": "~0.1.27-alpha",
<% } if (props.ui === 'bootstrap') { -%>
<%   if (props.target !== 'web') { -%>
    "fastclick": "^1.0.6",
<%   } -%>
    "font-awesome": "~4.7.0",
    "angular-bootstrap": "~2.5.0",
    "bootstrap-sass": "~3.3.6"
<% } if (props.ui === 'material') { -%>
    "font-awesome": "~4.6.2",
    "angular-material": "~1.0.8"
<% } if (props.ui === 'ionic') { -%>
    "ionic": "~1.3.0"
<% } -%>
  },
  "devDependencies": {
    "angular-mocks": "~1.6.2",
    "jquery": "~3.2.1",
    "jasmine-jquery": "~2.1.1"
  },
  "overrides": {
<% if (props.ui === 'bootstrap') { -%>
    "font-awesome": {
      "main": [
        "./scss/font-awesome.scss",
        "./fonts/*"
      ]
    },
    "bootstrap-sass": {
      "main": [],
      "dependencies": []
    }
  }
<% } if (props.ui === 'material') { -%>
    "font-awesome": {
      "main": [
        "./scss/font-awesome.scss",
        "./fonts/*"
      ]
    },
    "angular-material": {
      "main": [
        "./angular-material.js",
        "./angular-material.scss"
      ]
    }
  }
<% } if (props.ui === 'ionic') { -%>
    "ionic": {
      "main": [
        "release/fonts/*",
        "release/js/ionic.js",
        "release/js/ionic-angular.js"
      ]
    }
  },
  "resolutions": {
    "angular-ui-router": "~0.4.2",
    "angular-sanitize": "~1.6.2",
    "angular": "~1.6.2",
    "angular-animate": "~1.6.2"
  }
<% } -%>
}
